c Neutron Transport
phys:n 25 0 0 j j j 3  $force NCIA (no recoils)
FMULT METHOD=0 DATA=0 SHIFT=0
cut:n j j 0 0
c
c -----------------FOR 3HE --------------------------
mode n h t
c proton Transport
phys:h 20 3J 1
c triton Transport
phys:t 20 3J 1
cut:h  j 0.001
cut:t  j 0.001
c -----------------------------------------------------
C unidirectional monoenergetic thermal neutron source
sdef erg=.025e-6 pos=-20 0 0 vec 1 0 0 dir 1.0
c
c FOR 3He tallies ---------------------------------
f4:n 6
fm4 -1 36 103
sd4 1
f6:h 6               $ cell 6 is the gas
sd6 1 
f16:t 6               $ no energy bins needed
sd16 1
fc8 Sum of proton  and triton
f8:h 6                $ particle type and cell number are irrelevant
ft8 phl 2 6 1 16 1 0
e8 0 1e-5 25e-3 50e-3 75e-3 100e-3 75i 2
c
c -----------------------------------------------------------
f36:h,t 6
sd36 1
f208:n 6
ft208 cap 2003
f218:n 6
ft218 cap 2003 gate 150 2400
f228:n 6
ft228 cap edep 36 1e-3
