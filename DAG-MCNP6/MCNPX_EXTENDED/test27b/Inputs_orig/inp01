c
c **********************
c ****  CELL CARDS  ****
c **********************
c
 2000  700  -8.3800  -2000  2001    imp:h,n,p,e,d,t,s,a,#=1    $ brass collimator
 2001  102  -0.0012  -2001          imp:h,n,p,e,d,t,s,a,#=1    $ air in collimator
c
 3000 100 -1.00  -3000              imp:h,n,p,e,d,t,s,a,#=10   $ water phantom     -------- imp 10
c
8000 0 8000                         imp:h,n,p,e,d,t,s,a,#=0    $ outside universe (void)
8001 102 -0.0012 -8000                                         $ inside universe (air)
                 #2000                                         $ excluding collimator (brass)
                 #2001                                         $ excluding air in collimator (air)
                 #3000              imp:h,n,p,e,d,t,s,a,#=1    $ excluding water phantom (water)
c

c ****************************
c *****  SURFACE CARDS   *****
c ****************************
c
 2000  RPP  -15.00  15.00  -15.00  15.00  10.00 18.00   $ outside edge of collimator
 2001  RPP  -5.00  5.00  -5.00  5.00  10.00 18.00       $ inside edge of collimator
 8000 rcc 0 0 -100 0 0 200 100                          $ universe
 3000 rpp -25 25  -25 25  -50 0                         $ water phantom
c

